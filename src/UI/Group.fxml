<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="148.0" prefWidth="406.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.GroupController">
    <stylesheets>
        <URL value="@/Common/css/group.css" />
    </stylesheets>
   <children>
      <Button fx:id="btnCreateGroup" layoutX="41.0" layoutY="67.0" mnemonicParsing="false" onAction="#btnCreateGroupClicked" prefHeight="53.0" prefWidth="146.0" style="-fx-cursor: HAND;" text="CREATE GROUP">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow spread="0.37" />
         </effect>
      </Button>
      <Button fx:id="btnJoinGroup" layoutX="233.0" layoutY="67.0" mnemonicParsing="false" onAction="#btnJoinGroupClicked" prefHeight="53.0" prefWidth="146.0" style="-fx-cursor: HAND;" text="JOIN A GROUP">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow spread="0.37" />
         </effect>
      </Button>
      <Label layoutX="20.0" layoutY="14.0" prefHeight="31.0" prefWidth="308.0" text="PLEASE SELECT YOUR CHOISE" underline="true">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
   </children>
   <effect>
      <DropShadow blurType="ONE_PASS_BOX" color="WHITE" spread="0.5" />
   </effect>
</AnchorPane>
