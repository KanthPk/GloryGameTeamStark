<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="anchorJoinGame" prefHeight="246.0" prefWidth="549.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.Controller.JoinOnlineController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="264.0" text="JOIN ONLINE" underline="true">
         <font>
            <Font name="Nirmala UI" size="28.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="85.0" prefHeight="31.0" prefWidth="218.0" text="Group Name">
         <font>
            <Font name="System Bold" size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="cmbGroup" layoutX="221.0" layoutY="81.0" prefHeight="40.0" prefWidth="305.0" style="-fx-cursor: HAND;">
         <effect>
            <DropShadow spread="0.26" />
         </effect>
      </ComboBox>
      <Button fx:id="btnJoin" layoutX="381.0" layoutY="168.0" mnemonicParsing="false" onAction="#btnJoinClicked" prefHeight="53.0" prefWidth="145.0" text="JOIN">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow spread="0.37" />
         </effect>
      </Button>
      <Button fx:id="btnLeave" layoutX="221.0" layoutY="168.0" mnemonicParsing="false" onAction="#btnLeaveClicked" prefHeight="53.0" prefWidth="145.0" text="LEAVE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
         <effect>
            <DropShadow spread="0.37" />
         </effect>
      </Button>
   </children>
</AnchorPane>
